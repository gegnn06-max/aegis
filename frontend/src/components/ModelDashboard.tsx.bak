import { Card, CardContent, CardDescription, CardHeader, CardTitle } from "@/components/ui/Card";
import { Button } from "@/components/ui/button";
import { Badge } from "@/components/ui/badge";
import { Progress } from "@/components/ui/progress";
import { Tabs, TabsContent, TabsList, TabsTrigger } from "@/components/ui/tabs";
import { Play, Settings, BarChart3, Target, TrendingUp, Zap } from "lucide-react";

const ModelDashboard = () => {
  const metrics = {
    accuracy: 94.2,
    precision: 89.7,
    recall: 91.3,
    f1Score: 90.5,
    auc: 96.8
  };

  const baselineComparison = [
    { model: "GE-GNN (Ours)", f1: 90.5, auc: 96.8, recall: 91.3, status: "current" },
    { model: "GraphSAGE", f1: 84.2, auc: 91.4, recall: 82.7, status: "baseline" },
    { model: "GCN", f1: 82.1, auc: 89.9, recall: 80.1, status: "baseline" },
    { model: "GAT", f1: 85.7, auc: 92.8, recall: 84.2, status: "baseline" },
  ];

  return (
    <section className="py-16 px-6 bg-background">
      <div className="max-w-7xl mx-auto">
        <div className="text-center mb-12">
          <h2 className="text-3xl md:text-4xl font-bold mb-4">Model Performance</h2>
          <p className="text-xl text-muted-foreground max-w-2xl mx-auto">
            GE-GNN results and comparison with baseline Graph Neural Networks
          </p>
        </div>

        <Tabs defaultValue="overview" className="space-y-8">
          <TabsList className="grid grid-cols-2 w-[400px] mx-auto">
            <TabsTrigger value="overview">Overview</TabsTrigger>
            <TabsTrigger value="training">Training</TabsTrigger>
          </TabsList>

          <TabsContent value="overview" className="space-y-6">
            <div className="grid grid-cols-4 gap-4">
              {Object.entries(metrics).map(([key, value]) => (
                <Card key={key} className="bg-gradient-card border-primary/20">
                  <CardHeader className="pb-2">
                    <CardTitle className="text-sm font-medium">
                      {key.charAt(0).toUpperCase() + key.slice(1)}
                    </CardTitle>
                  </CardHeader>
                  <CardContent>
                    <div className="text-2xl font-bold">{value}%</div>
                  </CardContent>
                </Card>
              ))}
            </div>

            <Card className="bg-gradient-card border-primary/20 shadow-card">
              <CardHeader>
                <CardTitle>Baseline Model Comparison</CardTitle>
                <CardDescription>
                  Performance comparison with other graph neural networks
                </CardDescription>
              </CardHeader>
              <CardContent>
                <div className="space-y-4">
                  {baselineComparison.map((model) => (
                    <div key={model.model} className="space-y-2">
                      <div className="flex items-center justify-between">
                        <span className="font-medium">{model.model}</span>
                        <Badge
                          variant={model.status === "current" ? "default" : "secondary"}
                        >
                          {model.status}
                        </Badge>
                      </div>
                      <div className="grid grid-cols-3 gap-4 text-sm">
                        <div>
                          <span className="text-muted-foreground">F1-Score:</span>
                          <p className="font-semibold">{model.f1}%</p>
                        </div>
                        <div>
                          <span className="text-muted-foreground">AUC:</span>
                          <p className="font-semibold">{model.auc}%</p>
                        </div>
                        <div>
                          <span className="text-muted-foreground">Recall:</span>
                          <p className="font-semibold">{model.recall}%</p>
                        </div>
                      </div>
                    </div>
                  ))}
                </div>
              </CardContent>
            </Card>
          </TabsContent>

          <TabsContent value="training" className="space-y-6">
            <div className="grid lg:grid-cols-2 gap-6">
              <Card className="bg-gradient-card border-primary/20 shadow-card">
                <CardHeader>
                  <CardTitle className="flex items-center gap-2">
                    <Settings className="h-5 w-5 text-primary" />
                    Model Configuration
                  </CardTitle>
                </CardHeader>
                <CardContent className="space-y-4">
                  <div className="grid grid-cols-2 gap-4 text-sm">
                    <div>
                      <span className="text-muted-foreground">Hidden Dimensions:</span>
                      <p className="font-semibold">128</p>
                    </div>
                    <div>
                      <span className="text-muted-foreground">Attention Heads:</span>
                      <p className="font-semibold">8</p>
                    </div>
                    <div>
                      <span className="text-muted-foreground">GNN Layers:</span>
                      <p className="font-semibold">3</p>
                    </div>
                    <div>
                      <span className="text-muted-foreground">Dropout Rate:</span>
                      <p className="font-semibold">0.2</p>
                    </div>
                    <div>
                      <span className="text-muted-foreground">Learning Rate:</span>
                      <p className="font-semibold">0.001</p>
                    </div>
                    <div>
                      <span className="text-muted-foreground">Batch Size:</span>
                      <p className="font-semibold">512</p>
                    </div>
                  </div>
                </CardContent>
              </Card>

              <Card className="bg-gradient-card border-primary/20 shadow-card">
                <CardHeader>
                  <CardTitle>Edge Augmentation Settings</CardTitle>
                </CardHeader>
                <CardContent className="space-y-4">
                  <div className="grid grid-cols-2 gap-4 text-sm">
                    <div>
                      <span className="text-muted-foreground">Edge Features:</span>
                      <p className="font-semibold">64</p>
                    </div>
                    <div>
                      <span className="text-muted-foreground">Gating Alpha:</span>
                      <p className="font-semibold">0.3</p>
                    </div>
                    <div>
                      <span className="text-muted-foreground">Message Passing:</span>
                      <p className="font-semibold">Attention</p>
                    </div>
                    <div>
                      <span className="text-muted-foreground">Aggregation:</span>
                      <p className="font-semibold">Mean + Max</p>
                    </div>
                  </div>
                </CardContent>
              </Card>
            </div>

            <div className="flex gap-4 justify-center">
              <Button variant="hero" size="lg">
                <Play className="mr-2 h-5 w-5" />
                Start Training
              </Button>
              <Button variant="outline" size="lg">
                <Settings className="mr-2 h-5 w-5" />
                Modify Config
              </Button>
            </div>
          </TabsContent>
        </Tabs>
      </div>
    </section>
  );
};

export default ModelDashboard;